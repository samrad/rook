{
  "manifest_version": 2,
  "name": "Quick Tab Switch",
  "description": "Quickly switch between tabs by searching the titles and urls",
  "version": "1.0",
  "background": {
    "scripts": [
      "js/lcs.js",
      "js/model.js",
      "js/ui.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "hey title",
    "default_icon": "assets/icon.png",
    "default_popup": "html/popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+U",
        "mac": "Command+Shift+U",
        "chromeos": "Alt+U",
        "linux": "Ctrl+Shift+U"
      }
    }
  },
  "icons": {
    "16": "assets/mole-16.png",
    "48": "assets/mole-48.png",
    "128": "assets/mole-128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
